            <button id="bt1" class="btnn"
                style="width: 60px; height: 40px; background-color: white;color: red; border-radius: 5px; border: 1px solid grey;">1</button>
            <button id="bt2" class="btnn"
                style="width: 60px; height: 40px; background-color: white;color: red; border-radius: 5px; border: 1px solid grey;">2</button>
            <button id="bt3" class="btnn"
